# .env template for integration scripts
# Update these values as needed for your local Hardhat node and contracts

PRIVATE_KEY=your_local_account_private_key
CONTRACT_ADDRESS=deployed_proof_of_reserves_address
DAO_CONTRACT_ADDRESS=deployed_governance_dao_address
RPC_URL=http://127.0.0.1:8545

# For proposals/votes
PROPOSAL_DESCRIPTION=Test proposal
PROPOSAL_DATA=0x
PROPOSAL_ID=1
VOTE_SUPPORT=true

# For Merkle proof
MERKLE_ROOT=your_merkle_root
MERKLE_PROOF=["proof_hash1","proof_hash2"]
MERKLE_LEAF=your_leaf_hash
